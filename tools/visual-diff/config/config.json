{
  "masking": {
    "selectors": [
      ".ct-iframe",
      ".ct-map--canvas",
      ".ct-video-player",
      ".ct-video",
      "video"
    ]
  },
  "screenshot_options": {
    "disable_css_transitions": true,
    "hide_mask_selectors": true,
    "replace_images_with_solid_color": true,
    "settle_delay_ms": 2000
  },
  "screenshot_sources": {
    "current_branch": {
      "packages": {
        "twig": {
          "path": "packages/twig",
          "storybookPath": "packages/twig/storybook-static"
        },
        "sdc": {
          "path": "packages/sdc",
          "storybookPath": "packages/sdc/storybook-static"
        }
      }
    },
    "branches": {
      "main": {
        "packages": {
          "twig": {
            "path": "packages/twig",
            "storybookPath": "packages/twig/storybook-static"
          },
          "sdc": {
            "path": "packages/sdc",
            "storybookPath": "packages/sdc/storybook-static"
          }
        }
      }
    },
    "tags": {
      "v1.10.0": {
        "packages": {
          "twig": {
            "path": "components",
            "storybookPath": "storybook-static"
          }
        }
      }
    }
  }
}
